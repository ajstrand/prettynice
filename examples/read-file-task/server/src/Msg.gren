module Msg exposing (FileError(..), Msg(..))

import FileSystem exposing (AccessError, UnknownFileSystemError)
import Prettynice.Request exposing (Request)
import Prettynice.Response exposing (Response)


type FileError
    = OpenError AccessError
    | ReadError UnknownFileSystemError
    | DecodeError


type Msg
    = GotRequest Request Response
    | GotReadResult Response (Result FileError String)
